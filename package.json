{
        "name": "logsify",
        "version": "1.0.0",
        "description": "Modern logging dashboard with GitHub authentication",
        "main": "dist/server.js",
        "scripts": {
                "start": "node dist/server.js",
                "dev": "nodemon --exec ts-node src/server.ts",
                "build": "tsc",
                "watch": "tsc --watch",
                "css:watch": "tailwindcss -i src/styles/input.css -o public/css/styles.css --watch",
                "css:build": "tailwindcss -i src/styles/input.css -o public/css/styles.css --minify"
        },
        "keywords": [
                "logging",
                "dashboard",
                "github",
                "oauth",
                "typescript"
        ],
        "author": "",
        "license": "MIT",
        "dependencies": {
                "@types/ws": "^8.18.1",
                "axios": "^1.10.0",
                "cors": "^2.8.5",
                "crypto": "^1.0.1",
                "dotenv": "^16.3.1",
                "express": "^4.18.2",
                "express-session": "^1.17.3",
                "helmet": "^7.1.0",
                "mongoose": "^8.0.3",
                "morgan": "^1.10.0",
                "nunjucks": "^3.2.4",
                "passport": "^0.7.0",
                "passport-github2": "^0.1.12",
                "uuid": "^9.0.1",
                "ws": "^8.18.3"
        },
        "devDependencies": {
                "@types/cors": "^2.8.17",
                "@types/express": "^4.17.21",
                "@types/express-session": "^1.17.10",
                "@types/morgan": "^1.9.9",
                "@types/node": "^20.10.5",
                "@types/nunjucks": "^3.2.6",
                "@types/passport": "^1.0.16",
                "@types/passport-github2": "^1.2.9",
                "@types/uuid": "^9.0.7",
                "autocannon": "^8.0.0",
                "autoprefixer": "^10.4.16",
                "daisyui": "^4",
                "nodemon": "^3.0.2",
                "postcss": "^8.4.32",
                "tailwindcss": "^3",
                "ts-node": "^10.9.2",
                "typescript": "^5.3.3"
        }
}
